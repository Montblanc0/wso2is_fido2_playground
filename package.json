{
  "name": "wso2is_fido2_playground",
  "version": "1.0.0",
  "description": "A Passkey Manager that takes full advantage of WSO2IS FIDO2 API",
  "main": "server.js",
  "scripts": {
    "open-browser": "node -e \"require('dotenv').config(); require('child_process').exec('start ' + process.env.ORIGIN_URL + ':' + (process.env.PORT || 5500))\"",
    "start": "npm-run-all -p server open-browser",
    "server": "node ./server.js"
  },
