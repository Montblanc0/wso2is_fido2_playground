  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.1",
    "ejs": "^3.1.9",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "morgan": "^1.10.0",
    "npm-run-all": "^4.1.5",
    "open": "^10.0.3"
  }
}
